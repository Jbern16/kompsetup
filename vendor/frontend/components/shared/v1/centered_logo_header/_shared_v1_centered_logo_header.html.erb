<header class="centered-logo-header <%= @content['classes'] %>">
  <div class="inner-header">

    <div class="header-left">
      <div class="left-links">
        <% @content['left_links'].each do |link| %>
          <%= link_to link['text'], link['url'] || @cta_link %>
        <% end %>
      </div>
    </div>

    <div class="header-middle">
      <%= link_to(image_tag(@content['logo_img'], id: "header-logo"), @content['cta_link'] || @cta_link) %>
    </div>

    <div class="header-right">
      <%= link_to @content['cta_text'], @content['cta_link'] || @cta_link, class: "header-cta-link track-cta" %>
    </div>

  </div>
</header>
