<header class="header-with-links <%= @content['classes'] %>">
  <div class="inner-header">
    <%= link_to(image_tag(@content['img'], class: "header-logo"), @content['cta_link'] || @cta_link, class: "header-logo-link") %>

    <ul class="header-links">
      <% @content['links'].each do |header_link| %>
        <li><%= link_to header_link['text'], header_link['link'] || @cta_link %></li>
      <% end %>
    </ul>
  </div>
</header>