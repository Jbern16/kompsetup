<footer class="logo-footer <%= @content['classes'] %>">
 <div class="inner-footer">
   <%= link_to image_tag(@content['img']), @content['cta_link'] || @cta_link, class: "logo-link" %>
   <div class='footer-links'>
     <% @content['links'].each do |link| %>
        <%= link_to link['text'], link['url'] || @cta_link %>
      <% end %>
   </div>
 </div>
</footer>

